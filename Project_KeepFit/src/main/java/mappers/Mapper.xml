<!-- *********************************** [ MEMBER DB ] *********************************** -->
	
	<select id="loginMember" parameterType="member" resultType="member">
		<![CDATA[
  		SELECT * FROM member 
  		WHERE (member_email = #{member_email})
  			AND (member_pass = #{member_pass})
  		]]>
	</select>
	
	
	<insert id="insertMember" parameterType="member">
		<![CDATA[
 		INSERT INTO member
 		(member_id, member_email, member_pass, member_nick, member_channel, regdate) 
 		VALUES (#{member_id}, #{member_email}, #{member_pass}, #{member_nick}, 0, sysdate)		
 		
 		]]>
	</insert>
	
